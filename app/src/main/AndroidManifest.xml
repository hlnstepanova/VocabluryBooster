<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.estepanova.vocablurybooster">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LevelChoice"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".LanguageChoice"
            android:parentActivityName=".LevelChoice" />
        <activity
            android:name=".ModeChoice"
            android:parentActivityName=".LanguageChoice" />
        <activity
            android:name=".MainShow"
            android:parentActivityName=".ModeChoice">
            >
        </activity>
        <activity
            android:name=".TopicsChoice"
            android:parentActivityName=".ModeChoice" />
        <activity
            android:name=".WordCheck"
            android:parentActivityName=".ModeChoice" />
        <activity
            android:name=".CongratsTopic"
            android:label="You shall pass!" />
        <activity
            android:name=".Congrats"
            android:label="You shall pass!" />
        <activity
            android:name=".Feedback"/>
    </application>

</manifest>